<AnchorPane fx:id="rootAnchorPane" prefHeight="84.0" prefWidth="317.0" style="-fx-background-color: #424242;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="iconImageView" fitHeight="29.0" fitWidth="34.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/tasks_96px.png" />
         </image>
      </ImageView>
      <Label fx:id="taskLabel" layoutX="36.0" layoutY="2.0" prefHeight="25.0" prefWidth="246.0" text="Label" textFill="WHITE" wrapText="true">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="descriptionLabel" layoutX="36.0" layoutY="28.0" prefHeight="23.0" prefWidth="275.0" text="Description" textFill="WHITE" wrapText="true">
         <effect>
            <DropShadow />
         </effect></Label>
      <Label fx:id="dateViewLabel" alignment="CENTER" layoutY="61.0" prefHeight="23.0" prefWidth="326.0" style="-fx-background-color: #fff;" text=" date created" textFill="#32b594" />
      <ImageView fx:id="deleteButton" fitHeight="17.0" fitWidth="20.0" layoutX="294.0" layoutY="6.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/delete_forever_60px.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
